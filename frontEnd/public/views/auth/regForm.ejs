<link rel="stylesheet" href="./css/userRegisterForm.css" />
<body>
  <div class="form-container">
    <form action="" method="post">
      <h1>Register Form</h1>
      <div class="main-container">
        <div class="group">
          <label for="email" class="label">Email:</label>
          <input type="email" name="email" class="inputRegisterForm email" />
        </div>
        <div class="group">
          <label for="password" name="password" class="label">Password:</label>
          <input type="password" class="inputRegisterForm password" />
        </div>
      </div>
      <button type="submit" class="btn btn-login">Login</button>
    </form>
  </div>
</body>
